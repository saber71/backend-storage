{
  "name": "@heraclius/backend-storage",
  "version": "1.4.1",
  "private": true,
  "description": "提供了基于 Node.js 开发的 RESTful 接口，可以方便地进行数据的增删改查（CRUD）操作以及高效的数据搜索",
  "type": "module",
  "main": "dist_node/www.js",
  "scripts": {
    "build": "vite build && rimraf dist && pipenv run python setup.py sdist bdist_wheel",
    "watch": "vite build --watch",
    "test": "vitest run",
    "start": "node dist_node/www.js",
    "upload-python": "twine upload dist/*",
    "upload": "pnpm publish && pnpm run upload-python"
  },
  "devDependencies": {
    "@heraclius/backend-bridge": "^1.1.0",
    "@heraclius/collection": "^1.2.0",
    "@heraclius/external-deps-plugin": "^1.1.0",
    "@heraclius/http-test": "^1.1.0",
    "@heraclius/query": "^1.1.0",
    "@types/koa": "^2.15.0",
    "@types/koa-router": "^7.4.8",
    "@types/node": "^20.14.11",
    "@types/uuid": "^10.0.0",
    "koa": "^2.15.3",
    "koa-body": "^6.0.1",
    "koa-router": "^12.0.1",
    "unplugin-swc": "^1.5.1",
    "uuid": "^10.0.0",
    "vite": "^5.3.4",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^2.0.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@heraclius/backend-bridge": "^0.7.1",
    "@heraclius/collection": "^1.0.0",
    "@heraclius/query": "^1.0.0",
    "koa": "^2.15.3",
    "koa-body": "^6.0.1",
    "koa-router": "^12.0.1",
    "uuid": "^10.0.0"
  }
}
